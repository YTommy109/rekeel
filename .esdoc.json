{
    "source": "./src",
    "destination": "./doc/esdoc",
    "plugins": [
      {
        "name": "esdoc-standard-plugin",
        "option": {
          "manual": {
            "files": [
              "./manual/installation.md"
            ]
          },
          "test": {
            "source": "./src/",
            "interfaces": ["describe", "it", "context", "suite", "test"],
            "includes": ["(spec|Spec|test|Test)\\.js$"],
            "excludes": ["\\.config\\.js$"]
          }
        }
      },
      {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}}
    ]
  }
  